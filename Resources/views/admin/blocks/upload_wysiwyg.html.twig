{% if error is not defined %}
    <img src="{{ file_uploaded|media_url({'filter': 'admin'}) }}" data-source="{{ file_uploaded|media_url }}" alt=""/>
    <script type="text/javascript">
        window.parent.CKEDITOR.tools.callFunction(0, "{{ file_uploaded|media_url }}");
    </script>
{% else %}
    <script type="text/javascript">
        window.parent.uploadingError("{{ error }}");
    </script>
{% endif %}
