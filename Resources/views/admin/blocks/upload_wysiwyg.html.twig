{% if error is not defined %}
    <script type="text/javascript">
        window.parent.CKEDITOR.tools.callFunction(0, "{{ file_uploaded|media_url }}", "{{ file_uploaded.mime }}", "{{ file_uploaded.name }}");
    </script>
{% else %}
    <script type="text/javascript">
        window.parent.uploadingError("{{ error }}");
    </script>
{% endif %}
